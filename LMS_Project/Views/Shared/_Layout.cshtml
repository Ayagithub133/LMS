

<!DOCTYPE html>

<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>srtdash - ICO Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/png" href="~/Content/images/icon/favicon.ico">
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/themify-icons.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/metisMenu.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/slicknav.min.css" type="text/css">
    <!-- amchart css -->
    <link rel="stylesheet" href="https://www.amcharts.com/lib/3/plugins/export/export.css" type="text/css" media="all" />
    <!-- others css -->
    <link rel="stylesheet" href="~/Content/css/typography.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/default-css.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/styles.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/responsive.css" type="text/css">
    <!-- modernizr css -->

</head>
<body>
    @*<div id="preloader">
        <div class="loader"></div>
    </div>*@

   
        <!-- header area start -->
        <div class="header-area" style="background-color:#2980b9;width:100% !important">
            <div class="row align-items-center">
                <!-- nav and search button -->
                
                <!-- profile info & task notification -->
                <div class="col-md-6 col-sm-4 clearfix" >
                    <ul class="notification-area pull-right">

                        <li class="dropdown" style="color:#ffffff">
                            Blog
                        </li>
                        <li class="dropdown" style="color:#ffffff">
                            Contact Us

                        </li>
                        <li class="dropdown" style="color:#ffffff">
                            About Us

                        </li>
                        <li class="dropdown" style="color:#ffffff">
                            Home

                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- header area end -->

        <div style="margin-top:50px">
            @RenderBody()
        </div>
   

  

    <footer></footer>
    <script src="~/Scripts/js/vendor/modernizr-2.8.3.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/vendor/jquery-2.2.4.min.js" type="text/javascript"></script>
    <!-- bootstrap 4 js -->
    <script src="~/Scripts/js/popper.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/owl.carousel.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/metisMenu.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Scripts/js/jquery.slicknav.min.js" type="text/javascript"></script>

    <!-- start chart js -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js"></script>
    <!-- start highcharts js -->
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <!-- start zingchart js -->
    <script src="https://cdn.zingchart.com/zingchart.min.js"></script>
    <script>
        zingchart.MODULESDIR = "https://cdn.zingchart.com/modules/";
        ZC.LICENSE = ["569d52cefae586f634c54f86dc99e6a9", "ee6b7db5b51705a13dc2339db3edaf6d"];
    </script>
    <!-- all line chart activation -->
    <script src="~/Scripts/js/line-chart.js" type="text/javascript"></script>
    <!-- all pie chart -->
    <script src="~/Scripts/js/pie-chart.js" type="text/javascript"></script>
    <!-- others plugins -->
    <script src="~/Scripts/js/plugins.js" type="text/javascript"></script>
    <script src="~/Scripts/js/scripts.js"></script>
</body>
</html>

   